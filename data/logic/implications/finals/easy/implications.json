    {
      "question": "What is the first part of p→q called?",
      "answer": "antecedent",
      "alternatives": ["hypothesis"],
      "explanation": "Antecedent comes before arrow."
    },
    {
      "question": "What is the second part of p→q called?",
      "answer": "consequent",
      "alternatives": ["conclusion"],
      "explanation": "Consequent follows arrow."
    },
    {
      "question": "What switches p→q to q→p?",
      "answer": "converse",
      "alternatives": [],
      "explanation": "Converse swaps hypothesis and conclusion."
    },
    {
      "question": "What negates both parts of p→q?",
      "answer": "inverse",
      "alternatives": [],
